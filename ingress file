apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: myntra-ingress
  annotations:
    nginx.ingress.kubernetes.io/rewrite-target: /
spec:
  ingressClassName: nginx
  rules:
    - host: myntra.local    # Change this to your domain or use /etc/hosts locally
      http:
        paths:
          - path: /
            pathType: Prefix
            backend:
              service:
                name: myntra-service
                port:
                  number: 80

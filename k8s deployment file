apiVersion: apps/v1
kind: Deployment
metadata:
  name: myntra-deployment
spec:
  replicas: 1
  selector:
    matchLabels:
      app: myntra
  template:
    metadata:
      labels:
        app: myntra
    spec:
      containers:
      - name: myntra-container
        image: anithavalluri/myntra:v1
        ports:
        - containerPort: 80
